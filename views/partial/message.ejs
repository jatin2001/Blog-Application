<% if(typeof msg !='undefined') {%>
  <div class="alert alert-warning alert-dismissible fade show errormsg" role="alert">
     
    <p class="errormsg-font"><%=msg%></p>
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true" class="errormsg-font">&times;</span>
    </button>
  </div>
<%}%>
<% if(success_msg != ''){ %>
    <div class="alert alert-success alert-dismissible fade show errormsg" role="alert">
      <p class="errormsg-font"><%= success_msg %></p>
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true" class="errormsg-font">&times;</span>
      </button>
    </div>
    <% } %> 
<% if(error_msg != ''){ %>
    <div class="alert alert-danger alert-dismissible fade show errormsg" role="alert">
      <p class="errormsg-font"><%= error_msg %></p>
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true" class="errormsg-font">&times;</span>
      </button>
    </div>
 <% } %>
 <% if(error != ''){ %>
  <div class="alert alert-warning alert-dismissible fade show errormsg" role="alert">
    <p class="errormsg-font"><%= error %></p>
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true" class="errormsg-font">&times;</span>
    </button>
  </div>
<% }%>